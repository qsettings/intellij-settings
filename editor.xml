<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="12" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.1" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="chain-method.hints" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\bnote\b.*" />
    <pattern pattern="\bnotImplementederror\b.*" />
    <pattern pattern="\bbug\b.*" />
  </component>
</application>