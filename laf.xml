<application>
  <component name="LafManager" autodetect="true">
    <laf class-name="com.intellij.ide.ui.laf.darcula.DarculaLaf" themeId="ExperimentalDark" />
    <preferred-light-laf class-name="com.intellij.ide.ui.laf.IntelliJLaf" themeId="RiderDay" />
    <preferred-dark-laf class-name="com.intellij.ide.ui.laf.darcula.DarculaLaf" themeId="RiderNight" />
    <lafs-to-previous-schemes>
      <laf-to-scheme laf="Rider Day" scheme="_@user_Rider Light" />
      <laf-to-scheme laf="Rider Night" scheme="_@user_Default" />
    </lafs-to-previous-schemes>
  </component>
</application>